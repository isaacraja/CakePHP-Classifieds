	var $name = 'SpecialPage';
	var $belongsTo = array('User');
		var $hasMany = array(
			'Image' => array('className' => 'Image',
								'foreignKey' => 'special_page_id',
								'dependent' => false,
								'conditions' => '',
								'fields' => '',
								'order' => 'Image.id ASC',
								'limit' => '',
								'offset' => '',
								'exclusive' => '',
								'finderQuery' => '',
								'counterQuery' => ''
			)
	);
	
	var $actsAs = array('Sluggable'=> array('label'=>'product_offered','overwrite' => false));	
}
?>